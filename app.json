{
	"version": "1.0",
	"label": "Settings",
	"paths": ["settings"],
	"services": ["settings"],

	"architecture": {"engine": "starlark", "version": 2},
	"execute": [
		"settings.star",
		"user/account.star",
		"user/preferences.star",
		"system/settings.star",
		"system/users.star",
		"domains.star"
	],

	"actions": {
		"": {"file": "web/dist/index.html"},
		"assets": {"files": "web/dist/assets"},

		"user/account/data": {"function": "action_user_account"},
		"user/account/identity": {"function": "action_user_account_identity"},
		"user/account/sessions": {"function": "action_user_account_sessions"},
		"user/account/session/revoke": {"function": "action_user_account_session_revoke"},

		"user/preferences/data": {"function": "action_user_preferences"},
		"user/preferences/set": {"function": "action_user_preferences_set"},
		"user/preferences/reset": {"function": "action_user_preferences_reset"},

		"system/settings/data": {"function": "action_system_settings"},
		"system/settings/list": {"function": "action_system_settings_list"},
		"system/settings/get": {"function": "action_system_settings_get"},
		"system/settings/set": {"function": "action_system_settings_set"},

		"system/users/data": {"function": "action_system_users"},
		"system/users/list": {"function": "action_system_users_list"},
		"system/users/get": {"function": "action_system_users_get"},
		"system/users/create": {"function": "action_system_users_create"},
		"system/users/update": {"function": "action_system_users_update"},
		"system/users/delete": {"function": "action_system_users_delete"},

		"domains/data": {"function": "action_domains"},
		"domains/get": {"function": "action_domains_get"},
		"domains/update": {"function": "action_domains_update"},
		"domains/delete": {"function": "action_domains_delete"},
		"domains/route/create": {"function": "action_domains_route_create"},
		"domains/route/update": {"function": "action_domains_route_update"},
		"domains/route/delete": {"function": "action_domains_route_delete"},
		"domains/delegation/create": {"function": "action_domains_delegation_create"},
		"domains/delegation/delete": {"function": "action_domains_delegation_delete"},
		"domains/user/search": {"function": "action_domains_user_search"}
	}
}
